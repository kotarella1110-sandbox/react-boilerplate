{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "lint": "yarn lint:text && yarn lint:js && yarn lint:css",
    "lint:fix": "concurrently 'yarn lint:text --fix' 'yarn lint:js --fix' 'yarn lint:css --fix'",
    "lint:text": "textlint 'docs/**'",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.{css,scss}' './public/index.html'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addon-storyshots": "^3.4.8",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^1.0.0-rc.13",
    "jest-snapshot": "^23.4.1",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prh": "^5.4.3",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.5.2",
    "react-test-renderer": "^16.4.1",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-addon-smart-knobs": "^3.3.1",
    "stylelint": "^8.0.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^0.7.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "stylelint-scss": "^1.2.1",
    "textlint": "^10.2.1",
    "textlint-rule-preset-ja-technical-writing": "^3.0.1",
    "textlint-rule-prh": "^5.0.1",
    "textlint-rule-spellcheck-tech-word": "^5.0.0"
  },
  "resolutions": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1"
  }
